{
  "manifest_version": 3,
  "name": "Mass YouTube Unsubscriber",
  "version": "1.0",
  "description": "An extension to unsubscribe from multiple YouTube channels.",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
